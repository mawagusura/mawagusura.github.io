---
import PageLayout from "@/layouts/Base.astro";

const meta = {
	description: "Oops! It looks like this page is lost in space!",
	title: "Oops! You found a missing page!",
};
---

<PageLayout meta={meta}>
	<h1 class="title mb-6">404 | Seems like this page does not exist</h1>
	<p class="mb-8">
		Use the website navigation and/or the search bar to try and find your way around here
	</p>
</PageLayout>
